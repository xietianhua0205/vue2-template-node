{
  "name": "vue2-template",
  "version": "2.3.8",
  "private": true,
  "scripts": {
    "serve": "npm run get-token && vue-cli-service serve",
    "start": "node build/cleanToken.js && vue-cli-service serve",
    "build": "vue-cli-service build && node build/infoCopy.js",
    "build-studio": "npm run build && node build/buildZip.js",
    "build-report": "vue-cli-service build --report",
    "lint": "vue-cli-service lint",
    "lint:scss": "stylelint src/**/*.{html,vue,css,sass,scss}",
    "lint:less": "stylelint src/**/*.less --config .stylelintrc-less.js",
    "lint:style": "npm run lint:scss && npm run lint:less",
    "lintfix:style": "npm run lint:scss -- --fix && npm run lint:less -- --fix",
    "count-code": "node build/count ./src vue js less scss ts",
    "get-token": "node build/getToken.js",
    "lintfix": "eslint --ext .js,.ts,.vue \"src\" --fix",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "commit": "git-cz",
    "local-server": "nodemon --watch build --watch server/data server/server.js",
    "local-start": "cross-env START_TYPE=local vue-cli-service serve"
  },
  "dependencies": {
    "@plantdata/pd": "^1.0.0",
    "@plantdata/reactive-menu-item": "^0.0.61",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "dayjs": "^1.10.7",
    "element-ui": "2.15.7",
    "js-base64": "^3.7.2",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "qs": "^6.10.1",
    "vue": "~2.6.11",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "archiver": "^5.3.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "chalk": "^4.1.2",
    "cli-highlight": "^2.1.11",
    "commitizen": "^4.2.5",
    "commitlint-config-cz": "^0.13.3",
    "compression-webpack-plugin": "^5.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "express": "^4.17.1",
    "fs-extra": "^10.0.0",
    "husky": "^8.0.1",
    "ip": "^1.1.5",
    "less": "^3.13.1",
    "less-loader": "^7.3.0",
    "lint-staged": "^13.0.3",
    "monaco-editor-webpack-plugin": "^4.1.2",
    "nodemon": "^2.0.7",
    "postcss": "^8.4.20",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "postcss-scss": "^4.0.6",
    "request": "^2.88.2",
    "sass": "^1.45.0",
    "sass-loader": "^8.0.2",
    "stylelint": "^14.16.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-recommended-less": "^1.0.4",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-less": "^1.0.6",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "vue-cli-plugin-lodash": "~0.1.4",
    "vue-template-compiler": "~2.6.11",
    "zlib": "^1.0.5"
  },
  "lint-staged": {
    "{src,public}**/*.{js,vue}": [
      "npm run lint"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
