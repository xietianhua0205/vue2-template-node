{
  "name": "vue2-template",
  "version": "2.1.0",
  "private": true,
  "scripts": {
    "serve": "npm run get-token && vue-cli-service serve",
    "serve-layout-left": "npm run get-token && vue-cli-service serve --mode layout.left",
    "serve-layout-top": "npm run get-token && vue-cli-service serve --mode layout.top",
    "build": "vue-cli-service build && node build/infoCopy.js",
    "build-report": "vue-cli-service build --report",
    "lint": "vue-cli-service lint",
    "count-code": "node build/count ./src vue js less scss ts",
    "get-token": "node build/getToken.js",
    "lintfix": "eslint --ext .js,.ts,.vue \"src\" --fix",
    "prepare": "husky install",
    "server": "nodemon --watch build --watch server/data server/server.js"
  },
  "dependencies": {
    "@plantdata/reactive-menu-item": "0.0.52",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "dayjs": "^1.10.7",
    "element-ui": "^2.15.5",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "moment": "^2.29.1",
    "qs": "^6.10.1",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "archiver": "^5.3.0",
    "babel-eslint": "^10.1.0",
    "chalk": "^4.1.2",
    "cli-highlight": "^2.1.11",
    "compression-webpack-plugin": "^5.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "express": "^4.17.1",
    "husky": "^7.0.2",
    "ip": "^1.1.5",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "monaco-editor-webpack-plugin": "^4.1.2",
    "node-sass": "^4.12.0",
    "nodemon": "^2.0.7",
    "request": "^2.88.2",
    "sass-loader": "^8.0.2",
    "vue-cli-plugin-lodash": "~0.1.4",
    "vue-template-compiler": "^2.6.11",
    "zlib": "^1.0.5"
  }
}
